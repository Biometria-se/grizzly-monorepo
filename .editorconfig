# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{yml,yaml,json}]
indent_size = 2

[*.feature]
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[command-line-interface/tests/test_cli/unit/utils/test___init__.py]
trim_trailing_whitespace = false
